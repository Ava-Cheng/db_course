<%- include layouts/header.ejs %>

<div id="content">
    <%- include layouts/side.ejs %>
    <!-- 判斷是否有資料 -->
    <table id="member_table">
        <tr>
            <th style="width: 10%;padding-left: 5px;">會員編號</th>
            <th style="width: 30%">Email</th>
            <th style="width: 15%">姓名</th>
            <th style="width: 15%">生日</th>
            <th style="width: 20%">行動電話</th>
            <th style="width: 10%;padding-right: 20px;">性別</th>
        </tr>
        <% if(data[0]){ %>
            <% for(var i= 0;i < data.length;i++) { %>
                <tr class="member_table_tr">
                    <td class="td_center">
                        <%= data[i].User_No%>
                    </td>
                    <td class="td_center">
                        <%= data[i].Email%>
                    </td>
                    <td class="td_center">
                        <%= data[i].Name%>
                    </td>
                    <td class="td_center">
                        <input id="birth_cheng" value="<%= data[i].Birth%>" disabled="disabled"></input>
                    </td>
                    <td class="td_center">
                        <%= data[i].Phone%>
                    </td>
                    <td class="td_center" style="padding-right:15px;">
                        <%= data[i].Sex%>性
                    </td>
                </tr>
            <% } %>
        <% }else{%>
            <tr>
                <td colspan="6" class="td_center"><span class="nullData">沒有資料</span></td>
            </tr>
        <% } %>
    </table>
</div>

<%- include layouts/footer.ejs %>